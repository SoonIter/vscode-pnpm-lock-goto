{
  "publisher": "sooniter",
  "name": "pnpm-lock-goto",
  "displayName": "pnpm-lock-goto",
  "version": "0.0.0",
  "private": true,
  "packageManager": "pnpm@7.28.0",
  "description": "",
  "author": "sooniter <sooniter@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/SoonIter/vscode-pnpm-lock-goto#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/SoonIter/vscode-pnpm-lock-goto"
  },
  "bugs": {
    "url": "https://github.com/SoonIter/vscode-pnpm-lock-goto/issues"
  },
  "categories": [
    "Other"
  ],
  "main": "./dist/index.js",
  "icon": "assets/pnpm-lock.png",
  "files": [
    "dist"
  ],
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "onLanguage:yaml"
  ],
  "scripts": {
    "build": "tsup src/index.ts --external vscode",
    "dev": "nr build --watch",
    "lint": "eslint .",
    "vscode:prepublish": "nr build",
    "publish": "vsce publish --no-dependencies",
    "pack": "vsce package --no-dependencies",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "release": "bumpp && nr publish",
    "prepare": "simple-git-hooks"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.25.2",
    "@antfu/ni": "^0.17.2",
    "@commitlint/cli": "^17.4.4",
    "@commitlint/config-conventional": "^17.4.4",
    "@types/node": "^18.6.4",
    "@types/vscode": "^1.69.0",
    "bumpp": "^8.2.1",
    "eslint": "^8.21.0",
    "esno": "^0.16.3",
    "lint-staged": "^13.2.0",
    "pnpm": "^7.8.0",
    "rimraf": "^3.0.2",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.2.1",
    "typescript": "^4.7.4",
    "vite": "^3.0.4",
    "vitest": "^0.21.0",
    "vsce": "^2.10.0"
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/lint-staged",
    "commit-msg": "./node_modules/.bin/commitlint -e $HUSKY_GIT_PARAMS"
  },
  "lint-staged": {
    "*.{js,ts,tsx,vue,md}": [
      "eslint --fix"
    ]
  }
}